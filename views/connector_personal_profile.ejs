                                  <li><a class="dropdown-item" href="#">FD Calculator</a></li>
                                </ul>
                            </li>
                          </div>
                              <div class="login">
                                <span><a class="name-css " style="text-decoration: none; font-size: x-large; margin-right: 0.2rem;" href="/connector/profile"><%= locals.user.first_name %></a></span>
                                <button type="button" class="btn btn-danger ml-5" style="float: right;"><a class="display-name"  style="text-decoration: none; color: white;" href="/connector/sign-out">Sign Out</a></button>
                              </div>
